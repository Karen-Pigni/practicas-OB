    ],
    "author": "@Karen-Pigni",
    "license": "ISC",
    "devDependencies": {
        "@types/node": "^17.0.2",
        "eslint": "^8.38.0",
        "nodemon": "^2.0.15",
        "rimraf": "^3.0.2",
        "ts-node": "^10.4.0",
        "typedoc": "^0.24.4",
        "typescript": "^4.5.4"
    },
    "dependencies": {
        "cookies-utils": "^1.0.0"
    }
}
